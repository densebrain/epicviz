  "scripts": {
    "dev:electron": "ELECTRON_ARGS=\"[\\\"--remote-debugging-port=9223\\\",\\\"--inspect\\\"]\" node node_modules/.bin/electron-webpack dev",
    "dev": "concurrently \"npm:dev:electron\" \"npm:defs:watch\"",
    "compile": "npm run make-tern-defs && ross-env PACKAGE=true NODE_ENV=development electron-webpack",
    "build:mac": "npm run compile && electron-builder -c.compression=store \"-c.mac.identity=Densebrain, Inc\"",
    "build:linux": "npm run compile && electron-builder -l",
    "build:win": "npm run compile && electron-builder -w",
    "build:nosign": "npm run compile && electron-builder -c.compression=store -c.mac.identity=null",
    "build:dir": "npm run compile && electron-builder --dir -c.compression=store -c.mac.identity=null",
    "publish:mac": "npm run compile && electron-builder -c.compression=store \"-c.mac.identity=Densebrain, Inc\" --publish always",
    "publish:linux": "npm run compile && electron-builder -l --publish always",
    "publish:win": "npm run compile && electron-builder -w --publish always",
    "release": "npm version patch && npm run publish:mac && npm run publish:linux && npm run publish:win",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "postinstall": "rm -Rf $(find node_modules/@types/*/node_modules -name react)",
    "defs:compile": "tsc-transpile-only -p tsconfig.json && cd dist/out/src/common/context && ../../../../../node_modules/tern/bin/condense --name context --plugin modules --no-spans --plugin commonjs --plugin node --plugin node_resolve *Management.js > context.json && cd ../../../../../",
    "defs:watch": "node ./etc/scripts/context-generation.js"
  },
  "build": {
    "productName": "EpicViz",
    "mac": {
      "publish": [
        "github"
      ]
    }
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main",
      "webpackConfig": "etc/webpack/webpack.config.main.js"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "webpackConfig": "etc/webpack/webpack.config.renderer.js"
    }
  }
